<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.ContactsApp" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
        <item name="floatingActionButtonStyle">@style/CustomStyleFAB</item>

    </style>
    <style name="CustomStyleFAB" parent="@style/Widget.Material3.FloatingActionButton.Secondary">
        <item name="backgroundTint">@color/white</item>
        <item name="tint">@color/primaryColor</item>
    </style>

    <style name="Theme.ContactsApp_TextColor" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:textColorPrimary">@color/white</item>
    </style>


    <style name="Toolbar.Title" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
</resources>